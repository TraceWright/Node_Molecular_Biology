#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node --max-old-space-size=16000000000000"  "$basedir/../babel-cli/bin/babel-external-helpers.js" "$@"
  ret=$?
else 
  node --max-old-space-size=16000000000000  "$basedir/../babel-cli/bin/babel-external-helpers.js" "$@"
  ret=$?
fi
exit $ret
















